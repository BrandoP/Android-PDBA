<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main2Activity">
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/soulsearching_0000_layer1"
            android:contentDescription="MainScreen"
            android:scaleType = "centerCrop"
            android:id="@+id/imageView2"
            android:background="@drawable/soulsearching_0000_layer1"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
    />

    <ImageButton
            android:layout_width="38dp"
            android:layout_height="47dp" app:srcCompat="@drawable/icons8_about_26"
            android:id="@+id/imageButton"
            android:background="@null"
            android:alpha="0.60"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.989"
            app:layout_constraintVertical_bias="0.0" android:scaleType="fitCenter"/>

    <ImageButton
            android:layout_width="55dp"
            android:layout_height="53dp" app:srcCompat="@drawable/icons8_home_filled_50"
            android:id="@+id/imageButton2"
            android:background="@null"
            android:alpha="0.60"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"/>

</android.support.constraint.ConstraintLayout>